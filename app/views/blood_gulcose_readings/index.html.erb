<p id="notice"><%= notice %></p>

<h1>Blood Gulcose Readings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Value</th>
      <th>Reading type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blood_gulcose_readings.each do |blood_gulcose_reading| %>
      <tr>
        <td><%= blood_gulcose_reading.user_name %></td>
        <td><%= blood_gulcose_reading.value %></td>
        <td><%= blood_gulcose_reading.reading_type %></td>
        <td><%= link_to 'Show', blood_gulcose_reading %></td>
        <td><%= link_to 'Edit', edit_blood_gulcose_reading_path(blood_gulcose_reading) %></td>
        <td><%= link_to 'Destroy', blood_gulcose_reading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blood Gulcose Reading', new_blood_gulcose_reading_path %>
